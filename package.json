{
  "name": "eslint-plugin-meteor-redis-oplog-namespaces",
  "version": "0.0.0",
  "description": "Ensures MongoDB queries are namespaced when using meteor/cultofcoders:redis-oplog",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Nathan Muir",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha tests --recursive"
  },
  "dependencies": {
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^3.1.2"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "no-underscore-dangle": "off",
      "no-await-in-loop": "off",
      "no-restricted-syntax": "off"
    }
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "ISC"
}
